{
  "expo": {
    "name": "FCP Memorials",
    "slug": "FCP Memorials",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-qzf.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app-icon-qzf.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-qzf.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FCP Memorials to access your camera to scan QR codes at memorial sites"
        }
      ]
    ],
    "scheme": "FCP Memorials",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://zx9m4wtvay3c52kkt4rane79tkz3gpu2.app.specular.dev"
    }
  }
}