{
  "expo": {
    "name": "FCP Memorials",
    "slug": "FCP Memorials",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sunrise tree.JPG",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/sunrise tree.JPG",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fcpmemorials.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:fcpmemorials.com",
        "applinks:pwesm38m2s562pv8mecxsgwjqsk4267e.app.specular.dev"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sunrise tree.JPG",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fcpmemorials.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "fcpmemorials.com",
              "pathPrefix": "/go"
            },
            {
              "scheme": "https",
              "host": "fcpmemorials.com",
              "pathPrefix": "/memorial"
            },
            {
              "scheme": "https",
              "host": "fcpmemorials.com",
              "pathPrefix": "/auth"
            },
            {
              "scheme": "https",
              "host": "pwesm38m2s562pv8mecxsgwjqsk4267e.app.specular.dev",
              "pathPrefix": "/api/auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FCP Memorials to access your camera to scan QR codes at memorial sites"
        }
      ]
    ],
    "scheme": "FCP Memorials",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://fcpmemorials.com"
      },
      "backendUrl": "https://pwesm38m2s562pv8mecxsgwjqsk4267e.app.specular.dev"
    }
  }
}